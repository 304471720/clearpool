<?xml version="1.0" encoding="UTF-8"?>
<clearpool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="clearpool.xsd">
	<console>
		<port>8085</port>
		<security>
			<user>1</user>
			<password>1</password>
		</security>
	</console>
	<alias>jndiclearpool</alias>
	<jndi>
		<jndi-name>jndi</jndi-name>
		<prop key="java.naming.factory.initial">initial</prop>
		<prop key="java.naming.provider.url">url</prop>
		<prop key="java.naming.security.principal">user</prop>
		<prop key="java.naming.security.credentials">password</prop>
	</jndi>
	<core-pool-size>10</core-pool-size>
	<max-pool-size>50</max-pool-size>
	<acquire-increment>5</acquire-increment>
	<acquire-retry-times>3</acquire-retry-times>
	<limit-idle-time>5</limit-idle-time>
	<keep-test-period>20</keep-test-period>
	<test-table-name>test</test-table-name>
	<show-sql>false</show-sql>
</clearpool>